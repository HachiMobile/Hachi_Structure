{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "types": ["jest", "node"],
    "paths": {
      "@/*": ["./src/*"]
    },
    "jsx": "react",
    "strict": false,
  },
  "include": [
    "./src/**/*",
    "./src/@types/**/*",
    "index.d.ts",
    ".eslintrc.js",
    "jest.setup.js",
    "jest.config.js",
    "babel.config.js",
    "index.js",
    "react-native.config.js"
  ]
}
